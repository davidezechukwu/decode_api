		"axios": "1.8.3"
	},
	"scripts": {
		"prebuild": "npm run clean",
		"build": "tsc --build  --pretty",
		"build:rebuild": "npm run clean && npm install && npm run lint:fix && npm run build && npm run docs && npm run copy:all",
		"rebuild": "npm run build:rebuild",
		"postbuild": "npm run docs && npm run copy:all",
		"build:watch": "lb-tsc --watch",
		"build:prod": "tsc --build --verbose --listFiles --pretty",
		"clean": "lb-clean dist *.tsbuildinfo .eslintcache",
		"copy:all": "npm run copy:package:json && npm run copy:certificates && npm run copy:templates && npm run copy:casbin && npm run copy:views && npm run copy:public && npm run copy:env && npm run copy:xslt && npm run copy:swagger",
		"copy:casbin": "npx copyfiles --up 1  --all --verbose  ./src/_infrastructure/fixtures/casbin/* ./dist",
		"copy:package:json": "npx copyfiles --all --verbose ./package.json ./dist",
		"copy:env": "npx copyfiles --all --verbose ./.env ./dist",
		"copy:views": "npx copyfiles --up 1 --all --verbose ./src/_views/**/* ./dist",
		"copy:public": "npx copyfiles --up 1 --all --verbose ./src/_public/**/* ./dist",
		"copy:xslt": "npx copyfiles --up 1  --all --verbose  ./src/**/*.xslt ./dist",
		"copy:swagger": "npx copyfiles --up 1 --all --verbose ./src/swagger.html.ejs ./dist",
		"copy:certificates": "npx copyfiles --up 1  --all --verbose  ./src/_infrastructure/fixtures/certificates/* ./dist",
		"copy:templates": "npx copyfiles --up 1  --all --verbose  ./src/_infrastructure/fixtures/templates/**/* ./dist",
		"docker:build": "docker build -t api .",
		"docker:run": "docker run -p 3000:3000 -d api",
		"docs": "npx typedoc",
		"lint": "npm run lint:eslint:check --report-unused-disable-directives && npm run lint:prettier:check",
		"lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
		"lint:check": "npm run lint:eslint:check && npm run lint:prettier:check",
		"lint:eslint": "npx eslint  --debug --env-info --fix-dry-run --report-unused-disable-directives */**/*.ts",
		"lint:eslint:fix": "npx eslint --fix --report-unused-disable-directives */**/*.ts",
		"lint:eslint:check": "npx eslint  --debug --env-info --fix-dry-run --report-unused-disable-directives */**/*.ts",
		"lint:prettier:check": "npx prettier -c */**/*.ts",
		"lint:prettier": "npx prettier -c */**/*.ts",
		"lint:prettier:fix": "npx prettier --write */**/*.ts",
		"migrate:firstrun": "node ./_infrastructure/migrations/firstrun.js --rebuild",
		"migrate:resetdb-postgres": "psql.exe -h localhost -a  -U postgres -p 5432 -f ./_infrastructure/database/create_database_postgres.sql",
		"migrate-postgres": "psql.exe -h localhost -a  -U postgres -p 5432 -f ./_infrastructure/database/create_database_postgres.sql && npm run build && npm run migrate:firstrun",
		"migrate:resetdb-sqlserver": "echo not supported",
		"migrate-sqlserver": "npm run migrate:firstrun",
		"migrate-sqlserver-rebuild": "npm run build && npm run migrate:firstrun",
		"openapi-spec": "node ./openapi-spec",
		"preopenapi-spec": "npm run build",
		"start": "node dist/index.js",
		"test": "lb-mocha --allow-console-logs \"./_tests\"",
		"test:dev": "lb-mocha --allow-console-logs ./_tests/**/*.js && npm run posttest",
		"watch": "npm-watch",
		"watch:docs": "npm run docs",
		"watch:src": "npm run build"
	},
	"types": "index.d.ts",
	"version": "1.0.0.1",
	"watch": {
		"watch:docs": {
			"patterns": [
				"src"
			],
			"extensions": "ts,js",
			"quiet": false,
			"legacyWatch": true,
			"delay": 2500,
			"runOnChangeOnly": true
		},
		"watch:src": {
			"patterns": [
				"src"
			],
			"extensions": "ts",
			"quiet": false,
			"legacyWatch": true,
			"delay": 2500,
			"runOnChangeOnly": true
		}
	}
}
