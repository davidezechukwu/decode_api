	"knownIssues": [
		{
			"title": "The type for Model properties which are arrays are shown incorrectly on Swagger",
			"description": "The type for Model properties which are arrays are shown incorrectly as empty objects ({})",
			"link": "https://community.smartbear.com/t5/Swagger-Open-Source-Tools/Schema-with-an-Array-of-Objects-doesn-t-show-correct-object-type/m-p/206515#M1377"
		},
		{
			"title": "The dependency pug@3.0.2 has a breaking change",
			"description": "read plug-ins must now return Buffer if you want to support filters that use renderBuffer",
			"link": "https://github.com/pugjs/pug/releases/tag/pug%403.0.0"
		},
		{
			"title": "The dependency pug@3.0.2 has a breaking change",
			"description": "The minify option on filters now requires you to install the relevant js transformer",
			"link": "https://github.com/pugjs/pug/releases/tag/pug%403.0.0"
		},
		{
			"title": "The dependency xslt-processor should not exceed version 0.11.7",
			"description": "The dependency xslt-processor should not exceed version 0.11.7 as later versions of this package has breaking changes and bugs due to the asynchronous code introduced after this version",
			"link": "https://www.npmjs.com/package/xslt-processor"
		},
		{
			"title": "The dependency @loopback/eslint-config@15.0.5 has a peer dependency on eslint@8.57.x",
			"description": "Upgrading to a later version of eslint greater than version 8.57.2 is currently not possible because of this peer dependency",
			"link": "https://www.npmjs.com/package/xml-js"
		},
		{
			"title": "The dependency passport-twitter@0.1.x uses an older and broken version of the npm package xmldom",
			"description": "xmldom has multiple critical vulnerabilities, such as:- Misinterpreting malicious XML input and allowing multiple root nodes in a DOM",
			"link": "https://github.com/xmldom/xmldom/security/advisories/GHSA-h6q6-9hqw-rwfv"
		},
		{
			"title": "The dependency axios@1.8.3 has multiple high vulnerabilities",
			"description": "Axios@1.8.3 has a Cross-Site Request Forgery vulnerability  and requests are vulnerable To Possible SSRF and Credential Leakage via Absolute URL",
			"link": "https://github.com/advisories/GHSA-wf5p-g6vw-rhxx"
		}
	],
